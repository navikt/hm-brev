{
  "name": "hm-brev",
  "type": "commonjs",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development tsx watch --clear-screen=false --enable-source-maps -r dotenv/config src/index.ts",
    "build": "tsc -p .",
    "start": "NODE_ENV=production node --enable-source-maps -r dotenv/config dist/index.js",
    "prepare": "husky install",
    "prettier": "prettier . -c"
  },
  "dependencies": {
    "@portabletext/react": "3.0.4",
    "@sanity/client": "6.1.7",
    "axios": "1.4.0",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix src/"
    ]
  }
}
