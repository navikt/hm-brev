{
  "name": "hm-brev",
  "type": "commonjs",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development tsx watch --clear-screen=false --enable-source-maps -r dotenv/config src/index.ts",
    "build": "tsc -p .",
    "start": "NODE_ENV=production node --enable-source-maps -r dotenv/config dist/index.js",
    "prepare": "husky install",
    "prettier": "prettier . -c"
  },
  "dependencies": {
    "axios": "1.6.7",
    "express": "4.19.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@portabletext/react": "^3.0.11",
    "@sanity/client": "^6.14.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express-session": "^1.18.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix src/"
    ]
  }
}
